<Window x:Class="MemTrack.SortWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SortWindow" Height="194" Width="355">
    <Grid>
        <ComboBox x:Name="cb_Feilds" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="327" Loaded="cb_Feilds_Loaded" />
        <Button x:Name="btn_Sort" Content="Sort" HorizontalAlignment="Left" Margin="262,133,0,0" VerticalAlignment="Top" Width="75" Click="btn_Sort_Click"/>
        <StackPanel HorizontalAlignment="Left" Height="91" Margin="10,37,0,0" VerticalAlignment="Top" Width="327">
            <CheckBox x:Name="mi_YearFreshmen" Content="Freshmen"/>
            <CheckBox x:Name="mi_YearSoftmore" Content="Softmore"/>
            <CheckBox x:Name="mi_YearJunior" Content="Junior"/>
            <CheckBox x:Name="mi_YearSenior" Content="Senior"/>
        </StackPanel>

    </Grid>
</Window>
